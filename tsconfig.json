{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": ["esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "*": ["./typings/*"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext"
  }
}
