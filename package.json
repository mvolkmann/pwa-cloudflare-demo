{
  "name": "pwa-cloudflare-demo",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rm -rf public/scripts",
    "compile": "tsc",
    "build:release": "bun build --minify --splitting --outdir=public/scripts ./src/index.ts",
    "bundle-setup": "bun build --outdir ./public/scripts ./src/setup.ts",
    "bundle-sw": "bun build --outdir ./public/scripts ./src/bundle.ts",
    "dev": "wrangler dev src/server.tsx",
    "deploy": "wrangler deploy --minify src/index.tsx",
    "format": "prettier --write '**/*.{css,html,js,ts,tsx}'",
    "reinstall": "rm -rf node_modules bun.lockb && bun install",
    "test": "bun --watch test"
  },
  "dependencies": {
    "hono": "^3.12.7",
    "tiny-request-router": "^1.2.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/bun": "^1.0.4",
    "@types/web": "^0.0.137",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
